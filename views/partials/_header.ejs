<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse d-flex align-items-center justify-content-between" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
      </div>
    </div>
    <% if (user) { %>
      <div>
        <span> Logged in as: <%= user.email %> </span>
        <a href="/logout"><button type="submit" class="btn btn-primary"> Logout</button></a>
      </div>
      <% } else { %>
        <div>
          <a href="/register">
            <button class="btn btn-primary">Register</button>
          </a>
          <a href="/login">
            <button class="btn btn-primary">Login</button>
          </a>
        </div>
        <% } %>
  </nav>
</header>